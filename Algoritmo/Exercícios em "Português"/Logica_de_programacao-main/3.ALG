Algoritmo "exercicio3"
Var
   valor_hora, hora_trab, salario: real

Inicio
      escreval("Informe o valor pago por hora:")
      leia(valor_hora)
      escreval("Informe horas trabalhadas:")
      leia(hora_trab)
      salario:=valor_hora*hora_trab
      escreval("O professor deve receber: ",salario)
      

Fimalgoritmo